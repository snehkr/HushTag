{
  "version": 2,
  "builds": [
    {
      "src": "./main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "maxLambdaSize": "15mb",
        "installCommand": "pip install -r requirements.txt"
      }
    },
    {
      "src": "static/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}